ELSE
  echo "set afkick34005a500 *"0*"" >>T:_kickstartdefault

  echo "echo *"N/A    *" NOLINE >>T:kickstartmenu" >>T:_kickstartmenu
ENDIF
echo "echo *" : Cloanto AF Kickstart 1.3 (34.5) A500 rom file*" >>T:kickstartmenu" >>T:_kickstartmenu

