  echo "  echo *"Install*" NOLINE >>T:kickstartmenu" >>T:_kickstartmenu
  echo "ELSE" >>T:_kickstartmenu
  echo "  echo *"Skip   *" NOLINE >>T:kickstartmenu" >>T:_kickstartmenu
