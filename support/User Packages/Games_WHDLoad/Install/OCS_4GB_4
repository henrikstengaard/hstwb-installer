  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:E/EddTheDuck"
  Copy >NIL: "USERPACKAGEDIR:E/EddTheDuck" "$entrydir" ALL
ENDIF

