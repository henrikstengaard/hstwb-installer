; Copy 'Cygnus8' entry directory
IF EXISTS "USERPACKAGEDIR:C/Cygnus8.info"
  Copy >NIL: "USERPACKAGEDIR:C/Cygnus8.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "C/Cygnus8"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:C/Cygnus8"
  Copy >NIL: "USERPACKAGEDIR:C/Cygnus8" "$entrydir" ALL
ENDIF

; Copy 'Cytron' entry directory
