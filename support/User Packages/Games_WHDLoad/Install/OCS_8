  Copy >NIL: "USERPACKAGEDIR:I/Impact.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I/Impact"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I/Impact"
  Copy >NIL: "USERPACKAGEDIR:I/Impact" "$entrydir" ALL
ENDIF

; Copy 'Impact95' entry directory
