; Copy 'HardDrivin' entry directory
IF EXISTS "USERPACKAGEDIR:H/HardDrivin.info"
  Copy >NIL: "USERPACKAGEDIR:H/HardDrivin.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "H/HardDrivin"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:H/HardDrivin"
  Copy >NIL: "USERPACKAGEDIR:H/HardDrivin" "$entrydir" ALL
ENDIF

