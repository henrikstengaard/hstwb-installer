; Create 'I' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "I"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I.info"
  Copy >NIL: "USERPACKAGEDIR:I.info" "$indexdir"
ENDIF
echo "Installing 'I'..."

