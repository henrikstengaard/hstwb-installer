; Copy 'Virocop' entry directory
IF EXISTS "USERPACKAGEDIR:V/Virocop.info"
  Copy >NIL: "USERPACKAGEDIR:V/Virocop.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "V/Virocop"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:V/Virocop"
  Copy >NIL: "USERPACKAGEDIR:V/Virocop" "$entrydir" ALL
ENDIF

