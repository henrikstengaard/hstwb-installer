  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/PoliceQuest2"
  Copy >NIL: "USERPACKAGEDIR:P/PoliceQuest2" "$entrydir" ALL
ENDIF

