; Copy 'NorthAndSouthImage' entry directory
IF EXISTS "USERPACKAGEDIR:N/NorthAndSouthImage.info"
  Copy >NIL: "USERPACKAGEDIR:N/NorthAndSouthImage.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "N/NorthAndSouthImage"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:N/NorthAndSouthImage"
  Copy >NIL: "USERPACKAGEDIR:N/NorthAndSouthImage" "$entrydir" ALL
ENDIF

