  Copy >NIL: "USERPACKAGEDIR:O/OrkDemo.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "O/OrkDemo"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:O/OrkDemo"
  Copy >NIL: "USERPACKAGEDIR:O/OrkDemo" "$entrydir" ALL
ENDIF

