; Create 'D' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D.info"
  Copy >NIL: "USERPACKAGEDIR:D.info" "$indexdir"
ENDIF
echo "Installing 'D'..."

