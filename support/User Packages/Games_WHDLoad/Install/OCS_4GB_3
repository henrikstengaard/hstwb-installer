; Create 'E' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "E"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:E.info"
  Copy >NIL: "USERPACKAGEDIR:E.info" "$indexdir"
ENDIF
echo "Installing 'E'..."

; Copy 'EaglesRider' entry directory
IF EXISTS "USERPACKAGEDIR:E\EaglesRider.info"
  Copy >NIL: "USERPACKAGEDIR:E\EaglesRider.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:E\EaglesRider"
  Copy >NIL: "USERPACKAGEDIR:E\EaglesRider" "$indexdir" ALL
ENDIF

; Copy 'Ebonstar' entry directory
