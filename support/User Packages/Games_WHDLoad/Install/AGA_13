  Copy >NIL: "USERPACKAGEDIR:R/RoadWars.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "R/RoadWars"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R/RoadWars"
  Copy >NIL: "USERPACKAGEDIR:R/RoadWars" "$entrydir" ALL
ENDIF

