; Copy 'PowerPinball' entry directory
IF EXISTS "USERPACKAGEDIR:P/PowerPinball.info"
  Copy >NIL: "USERPACKAGEDIR:P/PowerPinball.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P/PowerPinball"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P/PowerPinball"
  Copy >NIL: "USERPACKAGEDIR:P/PowerPinball" "$entrydir" ALL
ENDIF

