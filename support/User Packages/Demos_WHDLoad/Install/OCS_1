  Copy >NIL: "USERPACKAGEDIR:D/Delirium.info" "$indexdir"
ENDIF
set entrydir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "D/Delirium"`"
IF NOT EXISTS "$entrydir"
  MakePath >NIL: "$entrydir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D/Delirium"
  Copy >NIL: "USERPACKAGEDIR:D/Delirium" "$entrydir" ALL
ENDIF

; Copy 'DeliriumDeform' entry directory
