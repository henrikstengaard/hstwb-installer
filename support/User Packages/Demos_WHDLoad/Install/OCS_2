; Create 'N' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "N"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:N.info"
  Copy >NIL: "USERPACKAGEDIR:N.info" "$indexdir"
ENDIF
echo "Installing 'N'..."

