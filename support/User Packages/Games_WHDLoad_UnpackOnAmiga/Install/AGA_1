; Create 'B' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "B"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:B.info"
  Copy >NIL: "USERPACKAGEDIR:B.info" "$INSTALLDIR"
ENDIF
echo "Installing 'B'..."

; Extract 'Battlestorm' entry archive
IF EXISTS "USERPACKAGEDIR:Battlestorm_v1.2_2008.zip"
  unzip -qq -o -x "USERPACKAGEDIR:Battlestorm_v1.2_2008.zip" -d "$indexdir"
ENDIF

; Extract 'BattlestormCDTV' entry archive
IF EXISTS "USERPACKAGEDIR:Battlestorm_v1.2_CDTV.zip"
  unzip -qq -o -x "USERPACKAGEDIR:Battlestorm_v1.2_CDTV.zip" -d "$indexdir"
ENDIF

